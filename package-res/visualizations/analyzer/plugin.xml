<?xml version='1.0' encoding='UTF-8'?>
<beans
  xmlns="http://www.springframework.org/schema/beans" 
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
           <!--  Analyzer Web Visualization -->
  <bean id="visualizationBean" class="org.pentaho.agilebi.pdi.visualizations.web.WebVisualization">
    <property name="title" value="Analyzer"/>
    <property name="extension" value="xanalyzer"/>
    <property name="newUrl" value="http://localhost:9999/pentaho/content/analyzer/editor?catalog=${modelLocation}&amp;cube=${modelId}&amp;showFieldList=true&amp;fieldListView=cmdViewType"/>
    <property name="openUrl" value="http://localhost:9999/pentaho/content/analyzer/editor?command=open&amp;showFieldList=true&amp;fieldListView=cmdViewType&amp;solution=&amp;path=${path}&amp;action=${filename}"/>
    <property name="saveJavascript" value="gCtrlr.repositoryBrowserController.remoteSave('${filename}', '', '${path}', null, true);"/>
    <property name="order" value="0"/>
  </bean>
</beans>